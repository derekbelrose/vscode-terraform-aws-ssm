{
  "name": "terraform-ssm",
  "displayName": "terraform-ssm",
  "description": "",
  "version": "0.0.1",
  "publisher": "itayadler",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:terraform"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.editSSMKey",
        "title": "terraform-ssm: Edit SSM Key"
      },
      {
        "command": "extension.switchAWSProfile",
        "title": "terraform-ssm: Switch AWS Profile"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "peerDependencies": {
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "async-shelljs": "^0.1.2",
    "aws-sdk": "^2.339.0",
    "bluebird": "^3.5.2",
    "ini": "^1.3.5",
    "shelljs": "^0.8.2",
    "ts-node": "^7.0.1",
    "typescript-require": "^0.2.10"
  }
}
